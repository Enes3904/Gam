import java.util.Scanner;

public class YapayZeka {

    public static void main(String[] args) {
        // Kullanıcıdan bir girdi al
        Scanner scanner = new Scanner(System.in);
        System.out.println("Bir girdi girin: ");
        String girdi = scanner.nextLine();

        // Giridi işle
        String cevap = "";
        switch (girdi) {
            case "merhaba":
                cevap = "Merhaba!";
                break;
            case "günaydın":
                cevap = "Günaydın!";
                break;
            case "iyi geceler":
                cevap = "İyi geceler!";
                break;
            default:
                cevap = "Anlamadım.";
                break;
        }

        // Cevap ver
        System.out.println(cevap);
    }
}

